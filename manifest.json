{
    "name": "Salem",
    "version": "1.2.2.8",
    "manifest_version": 3,
    "description": "Salem, asistente para ayudar a optimizar la documentaci√≥n de tickets de OTOBO en SOMI Estaciones.",
    "content_scripts": [
        {
            "matches": [
                "https://helpdesk.rbsas.co/otobo/index.pl*",
                "https://helpdesk.rbsas.co/otobo/index.pl*"
            ],
            "js": [
                "/sources/vendor/js/moment.min.js",
                "/sources/vendor/js/jquery-3.6.1.min.js",
                "/sources/vendor/js/sweetalert2@11.js",
                "/sources/api/salem.js"
            ],
            "css": [
                "sources/client/css/salem.css"
            ]
        }
    ],
    "background": {
        "service_worker": "/sources/api/background.js"
    },
    "icons": {
        "32": "icons/salem_32x32.png",
        "48": "icons/salem_48x48.png",
        "128": "icons/salem_128x128.png",
        "256": "icons/salem_256x256.png"
    },
    "action": {
        "default_icon": {
            "32": "icons/salem_32x32.png",
            "48": "icons/salem_48x48.png",
            "128": "icons/salem_128x128.png",
            "256": "icons/salem_256x256.png"
        }
    },
    "permissions": ["storage",  "tabs", "contextMenus"],
    "web_accessible_resources": [
        {
            "resources": [
                "*"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}